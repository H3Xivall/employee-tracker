{
    "env": {
      "es2021": true,
      "node": true,
      "commonjs": true,
    },
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 12,
    },
    "plugins": [
      "node",
      ""
    ],
    "rules": {
      "no-console": "off",
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "double"],
      "semi": ["error", "always"]
    }
  }
  